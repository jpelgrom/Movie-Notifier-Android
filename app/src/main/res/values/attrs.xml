<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DoubleRowIconPreferenceView">
        <attr name="detailIcon" format="integer"/>
        <attr name="detailTitle" format="string"/>
        <attr name="detailValue" format="string"/>
    </declare-styleable>

    <declare-styleable name="IconSwitchView">
        <attr name="switchIcon" format="integer"/>
        <attr name="switchTitle" format="string"/>
    </declare-styleable>

    <declare-styleable name="LicenseTextView">
        <attr name="licenseTitle" format="string"/>
        <attr name="licenseNotice" format="string"/>
        <attr name="licenseLicense" format="string"/>
    </declare-styleable>

    <!-- TODO https://issuetracker.google.com/issues/136103084 -->
    <attr name="flow_horizontalSeparator"/>
    <attr name="flow_verticalSeparator"/>
    <attr name="motionProgress"/>
    <attr name="waveDecay"/>
    <attr name="motionPathRotate"/>
    <attr name="duration"/>
</resources>